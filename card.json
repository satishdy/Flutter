{
  "_designCredit": "https://www.woolha.com/tutorials/flutter-card-widget-example",
  "type": "theme",
  "args": {
    "data": {
      "primaryColor": "#2c3ea4",
      "primaryColorBrightness": "dark"
    }
  },
  "child": {
    "type": "scaffold",
    "args": {
      "body": {
        "type": "center",
        "child": {
          "type": "container",
          "args": {
            "width": "419",
              "height": "175"
          },
          "child": {
            "type": "card",
            "args": {
              "color": "#E91E63",
              "elevation": 2,
              "shape": {
                "type": "rounded",
                "borderRadius": 8
              }
            },
            "child": {
              "type": "column",
              "args": {
                "mainAxisSize": "min"
              },
              "children": [
                {
                  "type": "button_bar",
                  "children": [
                    {
                      "type": "flat_button",
                      "args": {
                        "onPressed": "##noop()##"
                      },
                      "child": {
                        "type": "text",
                        "args": {
                          "text": "Edit",
                          "style": {
                            "color": "#fff"
                          }
                        }
                      }
                    },
                    {
                      "type": "flat_button",
                      "args": {
                        "onPressed": "##noop()##"
                      },
                      "child": {
                        "type": "text",
                        "args": {
                          "text": "Delete",
                          "style": {
                            "color": "#fff"
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          }
        }
      }
    }
  }
}
