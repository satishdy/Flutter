{
  "_designCredit": "https://www.woolha.com/tutorials/flutter-card-widget-example",
  "type": "theme",
  "args": {
    "data": {
      "primaryColor": "#2c3ea4",
      "primaryColorBrightness": "dark"
    }
  },
  "child": {
    "type": "scaffold",
    "args": {
      "appBar": {
        "type": "app_bar",
        "args": {
          "title": {
            "type": "text",
            "args": {
              "text": "Card"
            }
          }
        }
      },
      "body": {
        "type": "center",
        "child": {
          "type": "container",
          "args": {
            "width": "359",
            "height": "146"
          },
          "child": {
            "type": "card",
            "args": {
              "color": "#0A387A",
              "elevation": 5,
              "shape": {
                "type": "rounded",
                "borderRadius": 8
              }
            },
            "child": {
              "type": "column",
              "args": {
                "mainAxisSize": "min"
              },
              "children": [{
                "type": "list_tile",
                "args": {
                  "leading": {
                    "type": "network_image",
                    "args": {
                      "width": 106,
                      "height": 146,
                     "src": "https://firebasestorage.googleapis.com/v0/b/api-project-848950106722.appspot.com/o/IMG%20BG.png?alt=media&token=474ba69f-9edf-4eb4-84a5-87e8ee33855c"                      
                     }
                  },
                  "title": {
                    "type": "text",
                    "args": {
                      "text": "Shop Black Friday doorbusters and thousands of deals",
                      "textAlign": "center",
                      "style": {
                        "color": "#fff"
                      }
                    }
                  },
                  "subtitle": {
                    "type": "text",
                    "args": {
                      "text": "",
                      "style": {
                        "color": "#fff"
                      }
                    }
                  }
                }
              }, {
                "type": "button_bar",
                "children": [{
                  "type": "flat_button",
                  "args": {
                    "onPressed": "##noop()##"
                  },
                  "child": {
                    "type": "text",
                    "args": {
                      "text": "   Black Friday Deals   ",
                      "style": {
                        "color": "#555555",
                        "padding": 10,
                        "backgroundColor": "#fff"
                      }
                    }
                  }
                }, {
                  "type": "flat_button",
                  "args": {
                    "onPressed": "##noop()##"
                  },
                  "child": {
                    "type": "text",
                    "args": {
                      "text": "",
                      "style": {
                        "color": "#fff"
                      }
                    }
                  }
                }]
              }]
            }
          }
        }
      }
    }
  }
}
